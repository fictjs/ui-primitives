{
  "name": "@fictjs/ui-primitives",
  "version": "0.1.0",
  "description": "Official headless UI primitives for Fict",
  "keywords": [
    "fict",
    "fictjs",
    "ui",
    "headless",
    "primitives",
    "a11y"
  ],
  "license": "MIT",
  "type": "module",
  "packageManager": "pnpm@9.1.1",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "files": [
    "dist",
    "docs"
  ],
  "sideEffects": false,
  "engines": {
    "node": ">=18"
  },
  "peerDependencies": {
    "@fictjs/runtime": "^0.8.0"
  },
  "publishConfig": {
    "access": "public",
    "provenance": true
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/fictjs/ui-primitives.git"
  },
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "clean": "rimraf dist coverage examples/dist",
    "typecheck": "tsc --noEmit",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "test": "vitest run",
    "test:coverage": "vitest run --coverage",
    "test:watch": "vitest",
    "examples:dev": "vite --config examples/vite.config.ts",
    "examples:build": "vite build --config examples/vite.config.ts",
    "examples:preview": "vite preview --config examples/vite.config.ts",
    "examples:screenshots:install": "playwright install chromium",
    "examples:screenshots": "node scripts/capture-examples.mjs"
  },
  "devDependencies": {
    "@eslint/js": "^10.0.1",
    "@fictjs/runtime": "^0.8.0",
    "@testing-library/dom": "^10.4.1",
    "@types/node": "^25.2.2",
    "@vitest/coverage-v8": "^4.0.18",
    "eslint": "^10.0.0",
    "jsdom": "^28.0.0",
    "playwright": "^1.58.2",
    "prettier": "^3.8.1",
    "rimraf": "^6.1.2",
    "tsup": "^8.5.1",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.54.0",
    "vite": "^7.3.1",
    "vitest": "^4.0.18"
  }
}
